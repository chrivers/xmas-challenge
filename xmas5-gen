#!/usr/bin/python
import sys
from StringIO import StringIO

buf = StringIO()

def push_number(x):
    # push number
    buf.write("  ")
    # positive sign
    buf.write(" ")
    for c in bin(x)[2:].zfill(8).lstrip("0"):
        if c == "0":
            # zero
            buf.write(" ")
        else:
            # one
            buf.write("\t")
    # end parameter
    buf.write("\n")

def print_top_of_stack():
    # print top of stack
    buf.write("\t\n  ")

def pop():
    buf.write(" \n\n")

def addition():
    buf.write("\t   ")

def subtraction():
    buf.write("\t  \t")

def swap_top_of_stack():
    buf.write(" \n\t")

def duplicate_top_of_stack():
    buf.write(" \n ")

# flow control
def label(x):
    buf.write("\n  %s\n" % x)

def jump(label):
    buf.write("\n \n%s\n" % label)

def jump_if_zero(label):
    buf.write("\n\t %s\n" % label)

def jump_if_negative(label):
    buf.write("\n\t\t%s\n" % label)

def ret():
    buf.write("\n\t\n")

def end_program():
    buf.write("\n\n\n")

with file("full-lyrics.txt") as f:
    data = f.read()
    for c in reversed(data):
        push_number(ord(c)-ord("A"))
    push_number(len(data))

    label(" ")
    swap_top_of_stack()
    push_number(ord("A"))
    addition()
    print_top_of_stack()
    push_number(1)
    subtraction()
    duplicate_top_of_stack()

    jump_if_zero("  ")
    jump(" ")
    label("  ")
    end_program()

sys.stdout.write(buf.getvalue())
